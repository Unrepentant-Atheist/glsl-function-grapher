{
    "extends": "airbnb",
    "rules": {
        "indent": ["error", 4],
        "no-multi-spaces": ["error", { "exceptions": { "VariableDeclarator": true }}],
        "no-param-reassign": "off",
        "no-restricted-syntax": "off",
        "no-underscore-dangle": "off",
        "import/prefer-default-export": "off"
    },
    "env": {
        "browser": true,
        "jquery": true
    }
}
